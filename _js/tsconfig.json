{
	"compilerOptions": {
		"incremental": true,
		"target": "es2017",
		"module": "esnext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"rootDir": "src",
		"outDir": "../js/modules",
		"removeComments": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true
	},
	"include": ["src"]
}
